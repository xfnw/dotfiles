#!/usr/bin/env python3

import random

sortedletters = {' ': [' '], 'A': ['A', 'a', '·¥Ä', 'Ôº°', 'ÔΩÅ'], 'B': ['B', 'b', '√ü', ' ô', '·∫û', 'Ôº¢', 'ÔΩÇ'], 'C': ['C', 'c', ' ó', '·¥Ñ', 'Ôº£', 'ÔΩÉ'], 'D': ['D', 'd', '∆â', '·¥Ö', 'Ôº§', 'ÔΩÑ'], 'E': ['‚äò', 'E', 'e', '·¥á', 'Í¨≤', 'Í≠°', 'Ôº•', 'ÔΩÖ'], 'F': ['F', 'f', 'Í¨µ', 'Ôº¶', 'ÔΩÜ'], 'G': ['G', 'g', '…°', '…¢', 'Ôºß', 'ÔΩá'], 'H': ['H', 'h', ' ú', 'Ôº®', 'ÔΩà'], 'I': ['‚´Æ', 'I', 'i', 'ƒ±', '…™', 'Ôº©', 'ÔΩâ'], 'J': ['J', 'j', '»∑', '·¥ä', 'Ôº™', 'ÔΩä'], 'K': ['K', 'k', '·¥ã', 'Ôº´', 'ÔΩã'], 'L': ['L', 'l', ' ü', 'Ôº¨', 'ÔΩå'], 'M': ['M', 'm', '·¥ç', 'Ôº≠', 'ÔΩç'], 'N': ['«ã', 'N', 'n', '…¥', 'ÔºÆ', 'ÔΩé'], 'O': ['O', 'o', '·¥è', 'ÔºØ', 'ÔΩè'], 'P': ['P', 'p', '·¥ò', 'Ôº∞', 'ÔΩê'], 'Q': ['Q', 'q', 'Ôº±', 'ÔΩë'], 'R': ['R', 'r', ' Ä', 'Í≠à', 'Ôº≤', 'ÔΩí', '≈ø'], 'S': ['S', 's', 'Ôº≥', 'ÔΩì'], 'T': ['T', 't', '·¥õ', 'Ôº¥', 'ÔΩî'], 'U': ['U', 'u', '·¥ú', 'Ôºµ', 'ÔΩï'], 'V': ['V', 'v', '·¥†', 'Ôº∂', 'ÔΩñ'], 'W': ['W', 'w', '·¥°', 'Ôº∑', 'ÔΩó'], 'X': ['X', 'x', 'Ôº∏', 'ÔΩò'], 'Y': ['Y', 'y', ' è', 'Ôºπ', 'ÔΩô'], 'Z': ['Z', 'z', '«≤', '·¥¢', 'Ôº∫', 'ÔΩö'], '/': ['‚ÅÑ', '‚àï', '/', '‚ß∏', '‚πä', 'Ôºè', 'üôº'], '\\': ['\\', '‚ßπ', 'Ôπ®', 'Ôºº', 'üôΩ'], '!': ['ÔºÅ', '!'], '~': ['ÔΩû','~'], '#':['ÔºÉ','#'], '.':['.'], ',':[',']}

while True:

    inp = input().upper()

    for l in inp:
        print(random.choice(sortedletters[l] if l in sortedletters else l),end='',flush=True)
    print()
